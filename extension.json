{
	"name": "IfMobile",
	"version": "0.2.0",
	"author": [
		"Bumsakalaka"
	],
	"url": "",
	"descriptionmsg": "ifmobile-desc",
	"license-name": "BSD",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.39.0"
	},
	"MessagesDirs": {
		"IfMobile": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"IfMobileMagic": "IfMobile.i18n.magic.php"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\IfMobile\\": "src/"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "IfMobile"
	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\IfMobile\\Hooks"
		}
	},
	"Hooks": {
		"OutputPageAfterGetHeadLinksArray": {
			"handler": "main"
		},
		"ParserFirstCallInit": {
			"handler": "main"
		}
	},
	"load_composer_autoloader": true,
	"manifest_version": 2
}
